function reduce(fn){  CHARACTER_READ(cat("Macintosh HD_pb:applications: ACON:Overlays:BANQ_xyp:",fn),"DATA");  rec = len(data)[1];  ix=seq(1,rec);  for(i = 1; i <= (rec-1); i = i + 1) {    if((data[i;seq(1,20)] == data[i + 1;seq(1,20)])) then {      ix[i+1] = 0;    };  };      t=select(ix>0,ix);  data=data[t;];  MATRIX_WRITE(cat(0,"Macintosh HD_pb:applications: ACON:Overlays:BANQ_xyp:",fn,"2"),DATA);};reduce("gsd57eps.250xyp");reduce("gsd57eps.400xyp");reduce("gsd57eps.500xyp");reduce("gsd57eps.boundxyp");reduce("gsd59eps.125xyp");reduce("gsd59eps.250xyp");reduce("gsd59eps.400xyp");reduce("gsd59eps.500xyp");reduce("gsd59eps.700xyp");reduce("gsd59eps.boundxyp");